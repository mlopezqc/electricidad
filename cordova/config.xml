<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.electricitycuba" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <icon src="icon.png" width="57" height="57" density="mdpi" />
    <name>Electricidad Cuba</name>
    <description>
        Simple aplicación para calcular gastos de consumo eléctrico en Cuba
    </description>
    <author email="anonymous@gmail.com" href="https://anonymous.github.io/">
        Anonymous Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="android-minSdkVersion" value="22" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
